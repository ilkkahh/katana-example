<!-- 
 This is nuget package manager configuration file for SamWise project.
 Configuration options can be found from http://docs.nuget.org/docs/reference/nuget-config-settings
-->
<settings>   
    <config>
        <!-- Specifies where nuget downloads packages -->
        <add key="repositorypath" value="..\lib" />
    </config>    
    <repositoryPath>..\lib</repositoryPath>
    <packageRestore>
        <!-- Allow NuGet to download missing packages -->
        <add key="enabled" value="True" />

        <!-- Automatically check for missing packages during build in Visual Studio -->
        <add key="automatic" value="True" />
    </packageRestore>
    <packageSources>
        <!-- List of project's package sources. New package sources are added to this section -->
        <add key="NuGet official package source" value="https://nuget.org/api/v2/" />
    </packageSources>
    <disabledPackageSources />
    <activePackageSource>
        <!-- Specifies with package sources are used and which are not -->
        <add key="All" value="(Aggregate source)"  />
    </activePackageSource>    
</settings>